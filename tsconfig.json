{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "declaration": true,
      "jsx": "react-jsx",
      "declarationDir": "dist/types",
      "removeComments": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "moduleResolution": "node"
  
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }